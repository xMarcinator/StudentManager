@model IEnumerable<Student>

@if (@Model.Any())
{
    <div id="StudentList" class="row g-4 row-cols-1 row-cols-xl-4 row-cols-lg-3 row-cols-md-2">
         @foreach (var student in Model)
            {           
                <div class="col">
                    <partial name="studentComponent.cshtml" model="student"/>
                </div>
            }
    </div>
}
else
{
    <h1 style="text-align: center">No students Match search</h1>   
}